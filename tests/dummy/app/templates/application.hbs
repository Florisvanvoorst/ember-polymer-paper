<paper-drawer-panel>
  <paper-header-panel drawer>
    <paper-toolbar>
      <div>Drawer</div>
    </paper-toolbar>
    <paper-listbox selected="0">
      <paper-item>Item 1</paper-item>
      <paper-item>Item 2</paper-item>
    </paper-listbox>
  </paper-header-panel>
  <paper-header-panel main>
    <paper-toolbar>
      <paper-icon-button icon="menu" paper-drawer-toggle></paper-icon-button>
      <div>Ember Polymer Paper dummy app</div>
    </paper-toolbar>
    <div class="outlet">
      <h1>ember-polymer-paper</h1>
      {{outlet}}
    </div>
  </paper-header-panel>
</paper-drawer-panel>
